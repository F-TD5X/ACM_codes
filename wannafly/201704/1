int main()
{
    int n,m; scanf("%d%d",&n,&m);
    for (int i=1;i<=m;++i)
    {
        int k;
        scanf("%d",&k);
        s1.clear();
        int flag = 0;
        for (int j=1;j<=k;++j)
        {
            int a;
            scanf("%d",&a);
            if (s1.count(-a)) flag=1;
            s1.insert(a);
        }
        if (!flag)
        {
            puts("YES");
            return 0;
        }
    }
    puts("NO");
    return 0;
}
